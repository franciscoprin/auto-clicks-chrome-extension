{
  "name": "Auto Clicks",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "sesametimeBackground.js",
    "type": "module"
  },
  "host_permissions": [
    "*://app.sesametime.com/*"
  ],
  "minimum_chrome_version": "92",
  "permissions": [
    "tabs", 
    "storage",
    "alarms",
    "notifications",
    "activeTab",
    "identity",
    "identity.email",
    "webRequest",
    "webNavigation",
    "scripting"
  ]
}
